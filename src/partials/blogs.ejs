<div class="container">
<h4>My Blogs</h4>
<hr>
<% blog.forEach( blog =>{ %>
<div class="col-12 border" style="padding:0;">
<div class="row">
<div class="col-md-2" style="padding-top:10px;">
<center>
<img src="<%= blog.photo %>" class="rounded" style="width:90%"> 
</center>
</div>  
<div class="col-md-10" style="padding:20px;">
<h5><%= blog.title %></h5>  
<label><%= blog.short %></label><br><br>
<a href="/s/<%=blog.url%>/<%=blog.token%>"><button class="btn btn-dark">Görüntüle</button></a>
</div>
</div>
</div>
<% }) %>
</div>
