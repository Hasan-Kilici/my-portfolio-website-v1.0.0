<br>
  <h4>My Projects</h4>
  <div class="row">
    <% projects.forEach( projects =>{ %>
      <div class="col-md-3">
        <div class="card">
          <center>
          <img class="card-img-top" src="<%= projects.photo %>">
          </center>
          <div class="card-body">
            <h5><%= projects.title %></h5>
            <span><%= projects.description %></span>
          </div>
        <div class="card-footer">
         <form method="POST" action="/remove/project/<%= projects.token %>">
           <input type="submit" class="btn btn-danger" value="Sil">
          </form>  
        </div>
      </div>
    </div>
    <% }) %>
  </div>
